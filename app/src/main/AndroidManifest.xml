<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.meducation.mobile" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="net.meducation.mobile.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="net.meducation.mobile.ActivityTwo"
            android:label="@string/title_activity_activity_two"
            android:parentActivityName="net.meducation.mobile.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="net.meducation.mobile.MainActivity" />
        </activity>
        <activity
            android:name="net.meducation.mobile.ActivityThree"
            android:label="@string/title_activity_activity_three"
            android:parentActivityName="net.meducation.mobile.ActivityTwo" >
        </activity>
    </application>

</manifest>
